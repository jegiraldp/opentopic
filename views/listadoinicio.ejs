<!DOCTYPE html>
<html >
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>


    <div id="escondido">
          <% if (rta.length!=0) { %>
           <!-- <p class="menu">Cursos - <i>OpenTopic</i></p>-->
            <table id="tablaInicio">

            <% for(var i=0; i<rta.length; i++) { %>
              <tr class="curso">
                <td>
                    <a target="_blank" id="nombre" name="<%= rta[i].nombre %>">
                  <form id="f1">
                  <input type="text" id="txtNombreCurso" name="txtNombreCurso"
                  value="<%= rta[i].nombre %>"/>
                </form>
                </a></b>
               <br/> <%= rta[i].descripcion %> --
                 <br/><b>Autor:</b> <%= rta[i].creador %> - <%= rta[i].correo %>
                    - <b>Fecha:</b> <%= rta[i].fecha %>
                     <hr class="listado"/>
                    </td>
                    </tr>

            <% } %>
            </table>
          <% } else{ %>
            <i>-- Empty -- </i>
            <% } %>
        </div> <br/>
        <script src="js/indexListado.js"></script>

  </body>
</html>
