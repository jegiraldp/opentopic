<!DOCTYPE html>
<html >
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>


    <div id="escondido">
          <% if (rta.length!=0) { %>
           <!-- <p class="menu">Cursos - <i>OpenTopic</i></p>-->
            <table id="tablaInicio">

            <% for(var i=0; i<rta.length; i++) { %>
              <tr class="curso">
                <td>
               <b><a href="#" id="nombre" name="<%= rta[i].nombre %>">
                 <%= rta[i].nombre %></a></b>
               <br/> <%= rta[i].descripcion %> --
                 <br/><b>Creador :</b> <%= rta[i].creador %> - <%= rta[i].correo %>
                    - <b>Fecha:</b> <%= rta[i].fecha %>
                     <hr class="listado"/>
                    </td>
                    </tr>

            <% } %>
            </table>
          <% } else{ %>
            <i>-- Empty -- </i>
            <% } %>
        </div> <br/>
  </body>
</html>
